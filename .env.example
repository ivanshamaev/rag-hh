# Подключение к PostgreSQL (для запуска без Docker используйте localhost вместо db)
DATABASE_URL=postgresql://rag:rag@db:5432/rag_hh

# Модель эмбеддингов (384 размерности)
EMBEDDING_MODEL=sentence-transformers/paraphrase-multilingual-MiniLM-L12-v2

# Опционально: OAuth hh.ru (повышенные лимиты, меньше SSL/429) — см. https://dev.hh.ru/
# HH_CLIENT_ID=ваш_client_id
# HH_CLIENT_SECRET=ваш_client_secret
# HH_TOKEN=ваш_токен_bearer
